{"ts":1365534629714,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"$ ->\n  board = new App.Board 800, 600\n  player = new App.Player 400, 300, 2\n  game = new App.Game board, player\n  \n  group1 = new App.groupOfZombies 'group1'\n\n  for i in [0..100]\n    group1.addZombie new App.Zombie Math.random()*600, Math.random()*400, 1, 'test'+i\n  \n  board.addGroupOfZombies group1\n  \n    \n  game.animate()\n  \n  \n  player.moveTo 100, 100\n  \n  \n\n  \n  halmaOnClick = (e) ->\n    pos = getCursorPosition(e)\n    player.physicBody.ApplyImpulse({ x: (pos.x - player.positionX) * 100000, y: (pos.y - player.positionY) * 10000 }, player.physicBody.GetWorldCenter());\n    \n\n  getCursorPosition = (e) ->\n    if e.pageX or e.pageY\n      @x = e.pageX\n      @y = e.pageY\n    else\n      @x = e.clientX + document.body.scrollLeft + document.documentElement.scrollLeft\n      @y = e.clientY + document.body.scrollTop + document.documentElement.scrollTop\n      \n    return {x: @x, y: @y}\n      \n      \n  document.getElementsByTagName('canvas')[0].addEventListener(\"mousemove\", halmaOnClick, false)\n  \n  "]],"start1":0,"start2":0,"length1":0,"length2":1003}]],"length":1003}
